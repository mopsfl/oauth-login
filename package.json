{
  "name": "goofyluauglifier",
  "version": "2.1.1",
  "description": "Lua Uglifier",
  "scripts": {
    "start": "node ./dist/index.js prod",
    "prestart": "npm run build",
    "build": "rimraf dist && tsc && esbuild dist/index.js --platform=node --outdir=./dist --allow-overwrite --minify --bundle",
    "preserve": "npm run build",
    "serve": "concurrently \"tsc -w\" \"nodemon dist/index.js dev\"",
    "start_daki": "node ./dist/index.js"
  },
